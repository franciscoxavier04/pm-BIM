{
  "name": "openproject",
  "version": "0.1.0",
  "scripts": {
    "postinstall": "cd frontend && node \"$npm_execpath\" install",
    "test": "cd frontend && node \"$npm_execpath\" test && cd ..",
    "tslint_typechecks": "cd frontend && node \"$npm_execpath\" run tslint_typechecks && cd ..",
    "serve": "cd frontend && node \"$npm_execpath\" run serve",
    "serve:public": "cd frontend && ./node_modules/.bin/ng serve --host 0.0.0.0"
  },
  "private": true,
  "engines": {
    "node": "^22.15.0",
    "pnpm": "^10.13.1"
  },
  "devDependencies": {
    "@redocly/openapi-cli": "^1.0.0-beta.80"
  },
  "dependencies": {
    "@xeokit/xeokit-gltf-to-xkt": "^1.3.1"
  }
}
